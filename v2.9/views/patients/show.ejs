    <script>
      // Google Maps  
      $('#map-canvas').addClass('loading');
      var flightPlanCoordinates = [];
        
        //backend js starts
        <% currentRecords.forEach(function(record){ %>
           
           <% if(record.dateapp==20170604){ %>
                   <% for(var i = 0; i < record.coordinates.length;){ %>
                       flightPlanCoordinates.push({lat:<%=record.coordinates[i]%>, lng:<%=record.coordinates[i+1]%>});
                       <% i=i+2; %>
                   <% } %>
                     
           <% }; %>
        <% }); %>
        //backed js ends
        var settings = {
                          zoom: 15,
                          center: {lat: -37.7963689000 , lng: 144.9611738000},
                          mapTypeId: google.maps.MapTypeId.ROADMAP,
                          mapTypeControl: false,
                          scrollwheel: false,
                          draggable: true,
                          styles: [{"featureType":"landscape.natural","elementType":"geometry.fill","stylers":[{"visibility":"on"},{"color":"#e0efef"}]},{"featureType":"poi","elementType":"geometry.fill","stylers":[{"visibility":"on"},{"hue":"#1900ff"},{"color":"#c0e8e8"}]},{"featureType":"road","elementType":"geometry","stylers":[{"lightness":100},{"visibility":"simplified"}]},{"featureType":"road","elementType":"labels","stylers":[{"visibility":"off"}]},{"featureType":"transit.line","elementType":"geometry","stylers":[{"visibility":"on"},{"lightness":700}]},{"featureType":"water","elementType":"all","stylers":[{"color":"#7dcdcd"}]}],
                          mapTypeControlOptions: {style: google.maps.MapTypeControlStyle.DROPDOWN_MENU},
                          navigationControl: false,
                          navigationControlOptions: {style: google.maps.NavigationControlStyle.SMALL},            
                      };
                      var map = new google.maps.Map(document.getElementById("map-canvas"), settings);
                    
        
        var flightPath = new google.maps.Polyline({
          path: flightPlanCoordinates,
          geodesic: true,
          strokeColor: '#FF0000',
          strokeOpacity: 1.0,
          strokeWeight: 2
        });

        flightPath.setMap(map);

    </script>
    
