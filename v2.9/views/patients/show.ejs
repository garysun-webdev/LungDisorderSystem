    <script>
    <% function findNumOfFinished(array){ %>
            <% var number=0; %>
            <% array.forEach(function(arr){ %>
            <%  if(arr.distance>=arr.target){ %>
            <%       number++ %>
            <%   } %>
            <% }); %>
            <% return number; %>
        <% } %>
     
        <% if(currentRecords[0]==null){ %>
            <% var numFinished = 0; %> 
